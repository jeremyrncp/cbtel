<form method="post" action="{{ path('app_default_template_delete', {'id': default_template.id}) }}" onsubmit="return confirm('Êtes vous sûr de vouloir suppripmer cette entité ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ default_template.id) }}">
    <button class="btn btn-secondary">Delete</button>
</form>
